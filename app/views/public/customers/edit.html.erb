<h2>プロフィール編集</h2>
<%= form_with model: @customer, url:customers_my_page_path, method: :patch do |f| %>
  <label for="inputName">姓</label>
  <%= f.text_field :first_name, autofocus: true, id:"inputfirst_name", placeholder:"姓"%>
   <label for="inputName">名</label>
  <%= f.text_field :last_name, autofocus: true, id:"inputlast_name", placeholder:"名"%>
   <label for="inputName">姓</label>
  <%= f.text_field :post_code, autofocus: true, id:"inputpost_code", placeholder:"郵便番号"%>
   <label for="inputName">姓</label>
  <%= f.text_field :address, autofocus: true, id:"inputaddress", placeholder:"住所"%>
   <label for="inputName">姓</label>
  <%= f.text_field :telephone_number, autofocus: true, id:"inputtelephone_number", placeholder:"電話番号"%>
  <%= f.submit "変更を保存" %>
<% end %>

<%= link_to "退会", customers_confirmation_path %>