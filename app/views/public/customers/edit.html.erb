<h2>プロフィール編集</h2>
 
    <%= form_with model: @customer, url:customers_my_page_path, method: :patch do |f| %>
     <table>
      <tr><th>氏名</th><td><%= f.text_field :first_name, autofocus: true, id:"inputfirst_name", placeholder:"姓"%><span>　</span><%= f.text_field :last_name, autofocus: true, id:"inputlast_name", placeholder:"名"%></td></tr>
      <tr><th>カナ</th><td><%= f.text_field :first_name_kana, autofocus: true, id:"inputfirst_name_kana", placeholder:"姓カナ"%><span>　</span><%= f.text_field :last_name_kana, autofocus: true, id:"inputlast_name_kana", placeholder:"名カナ"%></td></tr>
      <tr><th>郵便番号</th><td><%= f.text_field :post_code, autofocus: true, id:"inputpost_code", placeholder:"郵便番号"%></td></tr>
      <tr><th>住所</th><td><%= f.text_field :address, autofocus: true, id:"inputaddress", placeholder:"住所"%></td></tr>
      <tr><th>電話番号</th><td><%= f.text_field :telephone_number, autofocus: true, id:"inputtelephone_number", placeholder:"電話番号"%></td></tr>
      <tr><th>メールアドレス</th><td><%= f.text_field :email, autofocus: true, id:"inputemail", placeholder:"メールアドレス"%></td></tr>
      <tr><td><%= f.submit "変更を保存" %></td></tr>
     </table>
    <% end %>

<%= link_to "退会", customers_confirmation_path %>
