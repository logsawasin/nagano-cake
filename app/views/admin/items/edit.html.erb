<h2>商品編集<h2>
        <%= form_with model: @item, url:admin_item_path, method: :patch do |f| %>
        <table>
          <tr><th>商品画像</th><td><%= f.file_field :image, accept: "image/*" %></td></tr>
          <tr><th>商品名</th><td><%= f.text_field :name %></td></tr>
          <tr><th>商品説明</th><td><%= f.text_field :text %></td></tr>
          <tr><th>税抜価格</th><td><%= f.text_field :without_tax_price %></td></tr>
          <tr><td><%= f.submit "変更を保存" %></td></tr>
        </table>
        <% end %>